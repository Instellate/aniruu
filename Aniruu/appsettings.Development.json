{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aniruu": "Trace"
    }
  },
  "OAuth2": {
    "GoogleRedirectUri": "http://localhost:5100/api/account/redirect",
    "DiscordRedirectUri": "http://localhost:5100/api/account/redirect"
  },
  "Limits": {
    "TagNameLimit": 50,
    "UsernameLimit": 25
  },
  "RateLimitRules": {
    "api/Account/claimUsername": {
      "ExpireTime": "1m",
      "IsPerUniqueUri": false,
      "RequestLimit": 20
    }
  }
}
